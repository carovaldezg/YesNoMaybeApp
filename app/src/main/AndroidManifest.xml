<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="caro.valdezg.yesnomaybeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme">
        <activity android:name=".googleLogin.GoogleLoginActivity"></activity>
        <activity android:name=".splashscreen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.HomeActivity"></activity>
        <activity android:name=".questionTab.QuestionActivity"></activity>
        <activity android:name=".about.AboutActivity"></activity>
    </application>

</manifest>
